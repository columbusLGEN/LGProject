#import "BaseModel.h"

@interface DynamicModel : BaseModel

/* 动态 */

/* 发布动态时间 */
@property (strong, nonatomic) NSString *date;
/* 好友信息 */
@property (strong, nonatomic) FriendModel *friendInfo;
/* 图书信息 */
@property (strong, nonatomic) BookModel *book;
/* 阅读总字数*/
@property (assign, nonatomic) NSInteger wordCount;
/* 阅读总天数*/
@property (assign, nonatomic) NSInteger dayCount;
/* 平均每天阅读字数*/
@property (assign, nonatomic) NSInteger averageWordCount;
/* 战胜了书友百分比*/
@property (assign, nonatomic) NSInteger victory;


@end
