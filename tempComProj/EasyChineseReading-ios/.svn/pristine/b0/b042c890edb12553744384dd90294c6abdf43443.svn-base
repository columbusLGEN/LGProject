#import "ECRFullminusModel.h"

static NSString *juan_selected = @"juan_selected";
static NSString *juan_available = @"juan_available";
static NSString *juan_unable = @"juan_unable";

@implementation ECRFullminusModel

- (NSString *)bgImgName{
    if (self.isAva) {
        if (self.isSelected) {
            return juan_selected;
        }else{
            return juan_available;
        }
    }else{
        return juan_unable;
    }
}

@end
