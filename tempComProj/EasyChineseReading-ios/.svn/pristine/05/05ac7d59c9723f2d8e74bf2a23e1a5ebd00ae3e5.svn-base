#import "BaseModel.h"

@interface FriendModel : BaseModel

/* 好友 */

@property (assign, nonatomic) NSInteger userId;     // 用户 id
@property (assign, nonatomic) NSInteger canview;    // 是否可以查看信息
@property (assign, nonatomic) NSInteger userType;   // 类型
@property (assign, nonatomic) NSInteger classId;    // 班级 id

@property (strong, nonatomic) NSString *name;
@property (strong, nonatomic) NSString *phone;
@property (strong, nonatomic) NSString *email;
@property (strong, nonatomic) NSString *iconUrl;

@property (assign, nonatomic) CGFloat readProgress; // 阅读进度

@property (strong, nonatomic) NSArray *readBook;    // ta正在读(books)
@property (strong, nonatomic) NSArray *historyBook; // 阅读历史(books)

@property (strong, nonatomic) NSArray *readHistory; // 阅读情况

@property (assign, nonatomic) BOOL isFriend; // 是我的好友

@end

