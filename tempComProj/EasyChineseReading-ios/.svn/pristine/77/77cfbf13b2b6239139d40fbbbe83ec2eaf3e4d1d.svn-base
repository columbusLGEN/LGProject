#import "BaseModel.h"

@interface UserModel : BaseModel

/**
 用户信息
 */

/* 用户类型*/
@property (strong, nonatomic) NSString *type;
/* 头像*/
@property (strong, nonatomic) NSString *headPic;
/* 昵称*/
@property (strong, nonatomic) NSString *name;
/* 居住地*/
@property (strong, nonatomic) NSString *address;
/* 性别*/
@property (strong, nonatomic) NSString *sex;
/* 母语*/
@property (strong, nonatomic) NSString *language;
/* 国家*/
@property (strong, nonatomic) NSString *country;
/* 学习汉语年数*/
@property (strong, nonatomic) NSString *learnyears;
/* 爱好*/
@property (strong, nonatomic) NSString *interest;
/* 生日*/
@property (strong, nonatomic) NSString *birthday;
/* 积分*/
@property (strong, nonatomic) NSString *integral;
/* 虚拟币*/
@property (strong, nonatomic) NSString *virtualCurrency;
/* 阅历id*/
@property (strong, nonatomic) NSString *readingTimeId;
/* 阅读总字数*/
@property (strong, nonatomic) NSString *wordCount;
/* 阅读总天数*/
@property (strong, nonatomic) NSString *dayCount;
/* 平均每天阅读字数*/
@property (strong, nonatomic) NSString *averageWordCount;
/* 战胜了书友百分比*/
@property (strong, nonatomic) NSString *victory;

/* 手机号 */
@property (strong, nonatomic) NSString *cellPhone;
/* email */
@property (strong, nonatomic) NSString *email;

@end
