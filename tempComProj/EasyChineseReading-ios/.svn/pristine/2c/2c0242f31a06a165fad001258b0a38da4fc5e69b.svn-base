#import "BaseModel.h"

@interface UserInfoModel : BaseModel

/**
 用户信息
 */

/* 用户类型*/
@property (assign, nonatomic) NSInteger type;
/* 头像*/
@property (strong, nonatomic) NSString *headPic;
/* 昵称*/
@property (strong, nonatomic) NSString *name;
/* 居住地*/
@property (strong, nonatomic) NSString *address;
/* 性别*/
@property (assign, nonatomic) NSInteger sex;
/* 母语*/
@property (strong, nonatomic) NSString *language;
/* 国家*/
@property (strong, nonatomic) NSString *country;
/* 学习汉语年数*/
@property (strong, nonatomic) NSString *learnyears;
/* 爱好*/
@property (strong, nonatomic) NSString *interest;
/* 生日*/
@property (strong, nonatomic) NSString *birthday;
/* 积分*/
@property (strong, nonatomic) NSString *integral;
/* 虚拟币*/
@property (assign, nonatomic) NSInteger virtualCurrency;
/* 阅历id*/
@property (assign, nonatomic) NSInteger readingTimeId;
/* 阅读总字数*/
@property (assign, nonatomic) NSInteger wordCount;
/* 阅读总天数*/
@property (assign, nonatomic) NSInteger dayCount;
/* 平均每天阅读字数*/
@property (assign, nonatomic) NSInteger averageWordCount;
/* 战胜了书友百分比*/
@property (assign, nonatomic) NSInteger victory;

@end
