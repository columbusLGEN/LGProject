#import <Foundation/Foundation.h>

typedef NS_ENUM(NSUInteger, ECRHomeClassModelClassType) {
    ECRHomeClassModelClassTypeEsb = 425,
    ECRHomeClassModelClassTypeCub = 446,
    ECRHomeClassModelClassTypeInb = 447,
    ECRHomeClassModelClassTypeHot = 1
};

@interface ECRHomeClassModel : NSObject

/**
3 汉语读物（第一个汉语读物分类） = 425,
4 文化读物 = 446,
2 互动教材 = 447

 */
@property (assign,nonatomic) NSInteger id;
@property (copy,nonatomic) NSString *name;//

/**
 type = 1 为分类
 type = 2 为系列
 
 汉语读物 = 1
 文化读物 = 2
 互动教材 = 2
 */
@property (assign,nonatomic) NSInteger type;//

@property (copy,nonatomic) NSString *mbDataKey;// 接口数据根据此属性跳转
@property (assign,nonatomic) NSUInteger classType;// 本地数据，根据此属性跳转，不能删除

@end
