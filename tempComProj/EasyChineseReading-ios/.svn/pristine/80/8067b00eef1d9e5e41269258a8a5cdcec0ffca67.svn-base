static NSString *ECRSubjectBookClickNotification = @"ECRSubjectBookClickNotification";

#import <Foundation/Foundation.h>

@interface ECRSubjectModel : NSObject

/*  图书id */
@property (assign, nonatomic) NSInteger bookId;
/*  封面 */
@property (strong, nonatomic) NSString *iconUrl;
/*  书名 */
@property (strong, nonatomic) NSString *bookName;
/*  作者 */
@property (strong, nonatomic) NSString *author;
/*  简介 */
@property (strong, nonatomic) NSString *contentValidity;
/*  评分（总评分） */
@property (assign, nonatomic) CGFloat score;
/*  价格 */
@property (assign, nonatomic) CGFloat price;

@end
